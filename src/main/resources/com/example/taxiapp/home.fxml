<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #303030;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.taxiapp.HomeController">
   <children>
      <Label layoutX="235.0" layoutY="40.0" text="Главная" textFill="WHITE">
         <font>
            <Font name="Arial Rounded MT Bold" size="35.0" />
         </font>
      </Label>
      <Button fx:id="editButton" layoutX="251.0" layoutY="139.0" mnemonicParsing="false" onAction="#editData" text="Внести данные">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Button fx:id="viewButton" layoutX="236.0" layoutY="239.0" mnemonicParsing="false" text="Просмотреть ремонт">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Button fx:id="addCar" layoutX="243.0" layoutY="189.0" mnemonicParsing="false" onAction="#add" text="Добавить машину">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
